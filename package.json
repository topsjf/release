{
  "name": "release",
  "version": "1.1.0",
  "description": "git推送规范，自动生成log文件、发布release",
  "scripts": {
    "changelog": "conventional-changelog -p atom -i CHANGELOG.md -s",
    "commit": "git-cz"
  },
  "devDependencies": {
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
